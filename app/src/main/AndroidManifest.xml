<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gambino_serra.condomanager_amministratore.tesi">

    <uses-permission android:name="com.damico.gianluca.sei.kiuapp.permission.MAPS_RECEIVE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission
        android:name="android.permission.CALL_PHONE"
        android:required="false" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name="com.gambino_serra.condomanager_amministratore.condomanager_amministratore"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CondoManager Amministratore"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.Login.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.Home.BachecaStabili.MappaStabili"
            android:label="@string/title_mappa_stabili"
            android:parentActivityName="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.Home.InterventiInCorso.BachecaInterventiInCorso.MappaInterventiInCorso"
            android:label="@string/title_mappa_interventi_in_corso"
            android:parentActivityName="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.Home.InterventiInCorso.InterventoInCorso.InterventoInCorso"
            android:label="@string/title_details_intervento"
            android:parentActivityName="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.Home.BachecaStabili.DettaglioStabile"
            android:label="@string/title_details_richiesta"
            android:parentActivityName="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.Home.BachecaNotifiche.InterventoCompletato.InterventoCompletato"
            android:label="@string/title_details_intervento"
            android:parentActivityName="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.Menu.StoricoInterventi.DettaglioInterventoArchiviato"
            android:label="@string/title_details_intervento"
            android:parentActivityName="com.gambino_serra.condomanager_amministratore.View.DrawerMenu.MainDrawer"
            android:screenOrientation="portrait" />

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name="com.gambino_serra.condomanager_amministratore.View.Home.InterventiInCorso.InterventoInCorso.RapportiIntervento.InserimentoRapportoIntervento" />
        <activity android:name="com.gambino_serra.condomanager_amministratore.View.Stabile.SezioneStabile"></activity>
    </application>

</manifest>